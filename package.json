{
  "name": "node_arch",
  "version": "0.0.0",
  "bin": {
    "architecture": "./cli.js"
  },
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec",
    "create:crud": "npx ts-node ./bin/cli.ts crud"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@services": "dist/app/services",
    "@repos": "dist/app/db/repos",
    "@shared": "dist/app/shared",
    "@server": "dist/server",
    "@routes": "dist/app/http/routes",
    "@util": "dist/app/util",
    "@controllers": "dist/app/http/controllers",
    "@models": "dist/app/db/models",
    "@type": "dist/app/types",
    "@http": "dist/app/http",
    "@validators": "dist/app/http/validators",
    "@middlewares": "dist/app/http/middlewares",
    "@views": "dist/app/views"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cluster": "^0.7.7",
    "command-line-args": "^6.0.1",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "mongoose": "^7.6.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "os": "^0.1.2",
    "plop": "^4.0.1",
    "redis": "^4.7.0",
    "redlock": "^5.0.0-beta.2",
    "validatorjs": "^3.22.1",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.16",
    "@types/validatorjs": "^3.15.0",
    "@types/winston": "^2.4.4",
    "@types/command-line-args": "^5.2.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18.19.62",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}
